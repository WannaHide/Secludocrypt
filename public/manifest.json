{
  "manifest_version": 2,

  "name": "Secludocrypt",
  "description": "Easily encrypt your online chats and posts... Everywhere!",
  "version": "1.0",
  "author": "HuckADuck",

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
        "default_title": "Secludocrypt",
        "default_icon": {
          "19":"icon.png",
          "20":"icon.png",
          "38":"icon.png"
        },  
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}